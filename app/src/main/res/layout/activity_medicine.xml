<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    tools:context=".medicine"

    >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:orientation="vertical"
        android:id="@+id/containerLayout">
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="4dp"
            app:cardElevation="2dp"
            app:cardUseCompatPadding="true"
            app:contentPadding="16dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/imageViewSave"
                        android:layout_width="32dp"
                        android:layout_height="36dp"
                        android:src="@drawable/save"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.95"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="1.0" />


                </androidx.constraintlayout.widget.ConstraintLayout>



            </LinearLayout>
        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="4dp"
            app:cardElevation="2dp"
            app:cardUseCompatPadding="true"
            app:contentPadding="16dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Medicine Name:"
                    android:fontFamily="@font/poppinsmedium"
                    android:textColor="@color/deep_sapphire"
                    />
                <EditText
                    android:id="@+id/edit_med_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Enter medicine name" />

            </LinearLayout>
        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            app:cardCornerRadius="4dp"
            app:cardElevation="2dp"
            app:cardUseCompatPadding="true"
            app:contentPadding="16dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/startdate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="start date"
                        android:textColor="@color/deep_sapphire"
                        android:drawableLeft="@drawable/calender"
                        android:textSize="17sp"
                        android:gravity="center"
                        android:drawableBottom="@drawable/underline"
                        />


                    <TextView
                        android:id="@+id/enddate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableLeft="@drawable/calender"
                        android:textColor="@color/deep_sapphire"
                        android:layout_marginLeft="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:hint="end date"
                        android:drawableBottom="@drawable/underline"/>



                </LinearLayout>


            </LinearLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            app:cardCornerRadius="4dp"
            app:cardElevation="2dp"
            app:cardUseCompatPadding="true"
            app:contentPadding="16dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Medicine Days:"
                    android:fontFamily="@font/poppinsmedium"
                    android:textColor="@color/deep_sapphire"
                    />


                <CheckBox
                    android:id="@+id/every_day"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="methodOnClick"
                    android:fontFamily="@font/poppinsmedium"
                    android:text="Everyday" />

                <LinearLayout
                    android:id="@+id/checkbox_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="7">

                    <CheckBox
                        android:id="@+id/sunday"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_margin="4dp"
                        android:fontFamily="@font/poppinsmedium"
                        android:layout_weight="1"
                        android:onClick="methodOnClick"
                        android:button="@null"
                        android:gravity="center"
                        android:buttonTint="@color/deep_sapphire"
                        android:text="S" />

                    <CheckBox
                        android:id="@+id/monday"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_margin="4dp"
                        android:layout_weight="1"
                        android:button="@null"
                        android:buttonTint="@color/deep_sapphire"
                        android:fontFamily="@font/poppinsmedium"
                        android:onClick="methodOnClick"
                        android:gravity="center"
                        android:text="M" />


                    <CheckBox
                        android:id="@+id/tuesday"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_margin="4dp"
                        android:layout_weight="1"
                        android:button="@null"
                        android:buttonTint="@color/deep_sapphire"
                        android:onClick="methodOnClick"
                        android:gravity="center"
                        android:text="T" />

                    <CheckBox
                        android:id="@+id/wed"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:buttonTint="@color/deep_sapphire"
                        android:layout_margin="4dp"
                        android:fontFamily="@font/poppinsmedium"
                        android:layout_weight="1"
                        android:button="@null"
                        android:onClick="methodOnClick"
                        android:gravity="center"
                        android:text="W"/>

                    <CheckBox
                        android:id="@+id/thurs"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_margin="4dp"
                        android:buttonTint="@color/deep_sapphire"
                        android:layout_weight="1"
                        android:fontFamily="@font/poppinsmedium"
                        android:button="@null"
                        android:onClick="methodOnClick"
                        android:gravity="center"
                        android:text="Th" />

                    <CheckBox
                        android:id="@+id/fri"
                        android:layout_width="40dp"
                        android:buttonTint="@color/deep_sapphire"
                        android:layout_height="40dp"
                        android:layout_margin="4dp"
                        android:layout_weight="1"
                        android:button="@null"
                        android:fontFamily="@font/poppinsmedium"
                        android:onClick="methodOnClick"
                        android:gravity="center"
                        android:text="F" />


                    <CheckBox
                        android:buttonTint="@color/deep_sapphire"
                        android:id="@+id/sat"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_margin="4dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/poppinsmedium"
                        android:button="@null"
                        android:gravity="center"
                        android:onClick="methodOnClick"
                        android:text="Sa" />
                </LinearLayout>
            </LinearLayout>

        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            app:cardCornerRadius="4dp"
            app:cardElevation="2dp"
            app:cardUseCompatPadding="true"
            app:contentPadding="16dp"
            android:id="@+id/medicinecard">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Reminder"
                    android:fontFamily="@font/poppinsmedium"
                    android:textColor="@color/deep_sapphire"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/medicine_time0"
                        android:layout_width="110dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:drawableBottom="@drawable/underline"
                        android:gravity="center"
                        android:hint="10:00"
                        android:inputType="datetime"
                        android:padding="4dp"
                        android:textSize="17sp"
                        android:textStyle="bold" />


<!--                    <EditText-->
<!--                        android:id="@+id/tv_dose_quantity"-->
<!--                        android:layout_width="wrap_content"-->
<!--                        android:layout_height="wrap_content"-->
<!--                        android:layout_marginStart="8dp"-->
<!--                        android:layout_marginLeft="8dp"-->
<!--                        android:layout_weight="1"-->
<!--                        android:gravity="center"-->
<!--                        android:hint="1.0"-->
<!--                        android:padding="4dp" />-->

                    <androidx.appcompat.widget.AppCompatSpinner
                        android:id="@+id/spinner_dose_units"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_weight="1"
                        android:entries="@array/medications_shape_array" />
                </LinearLayout>


            </LinearLayout>
        </androidx.cardview.widget.CardView>
        <Button
            android:id="@+id/add_field_button"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="10dp"
            android:background="#555"
            android:layout_gravity="center"
            android:onClick="onAddField"
            android:textColor="#FFF"
            android:text="Add Field"
            android:paddingLeft="5dp"/>


